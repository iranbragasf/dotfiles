Host github.com
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github

Host github-mblabs
    HostName github.com
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github_mblabs

Host github-bullla
    HostName github.com
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github_bullla

Host bitbucket.org
    AddKeysToAgent yes
    IdentityFile ~/.ssh/bitbucket_mblabs